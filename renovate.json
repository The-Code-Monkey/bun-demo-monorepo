{
  "extends": [
    ":dependencyDashboard",
    ":semanticPrefixFixDepsChoreOthers",
    ":ignoreModulesAndTests",
    ":autodetectPinVersions",
    "group:monorepos",
    "group:recommended",
    "workarounds:all"
  ],
  "rangeStrategy": "pin",
  "prConcurrentLimit": 3,
  "labels": ["dependencies"],
  "vulnerabilityAlerts": {
    "labels": ["security"],
    "enabled": true
  },
  "updateInternalDeps": true
}
